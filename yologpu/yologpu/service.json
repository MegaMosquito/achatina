{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [
    { "org": "$HZN_ORG_ID", "url": "yolov3", "version": "1.0.1", "arch": "$ARCH" },
    { "org": "$HZN_ORG_ID", "url": "mqtt", "version": "1.0.1", "arch": "$ARCH" }
  ],
  "userInput": [
    {
      "label": "",
      "name": "EVENTSTREAMS_BROKER_URLS",
      "type": "string",
      "defaultValue": ""
    },
    {
      "label": "",
      "name": "EVENTSTREAMS_API_KEY",
      "type": "string",
      "defaultValue": ""
    },
    {
      "label": "",
      "name": "EVENTSTREAMS_PUB_TOPIC",
      "type": "string",
      "defaultValue": ""
    }
  ],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION"
      }
    }
  }
}

